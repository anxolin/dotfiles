{
  "workbench.colorTheme": "One Dark Pro",
  "editor.minimap.enabled": false,
  "eslint.packageManager": "yarn",
  "window.zoomLevel": 1,
  "diffEditor.ignoreTrimWhitespace": true,
  "explorer.openEditors.visible": 0,
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.python"
  },
  "[solidity]": {
    "editor.tabSize": 4
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "python": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "editor.detectIndentation": false,
  "[json]": {
    "editor.folding": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.rulers": [120],
  "explorer.confirmDragAndDrop": false,
  "editor.folding": false,
  "workbench.editor.enablePreview": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.history": true,
    "**/.retry": true
  },
  "solidity.soliumRules": {
    "imports-on-top": 0,
    "variable-declarations": 0,
    "indentation": ["error", 4],
    "quotes": ["error", "double"]
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "solidity.linter": "solium",
  "solidity.packageDefaultDependenciesContractsDirectory": "",
  "editor.tabSize": 2,
  "breadcrumbs.enabled": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "es": true,
    "lib": true
  },
  "liveshare.audio.joinCallBehavior": "accept",
  "files.associations": {
    ".env": "shellscript"
  },
  "rust.clippy_preference": "on",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "cSpell.userWords": [
    "Etherscan",
    "Mainnet",
    "Rinkeby",
    "angelrf",
    "behaviour",
    "ethereum",
    "goerli",
    "kovan",
    "ropsten"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
    // "source.organizeImports": true
  },
  "explorer.confirmDelete": false,
  "python.jediEnabled": false,
  "editor.formatOnSave": true,
  "todo-tree.tree.showScanModeButton": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "python.formatting.provider": "black",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:updated-desc"
    },
    {
      "label": "Milestone: 15",
      "query": "state:open repo:${owner}/${repository} milestone:\"Sprint 15\" sort:updated-desc"
    },
    {
      "label": "Bugs",
      "query": "state:open repo:${owner}/${repository} label:bug sort:updated-desc"
    },
    {
      "label": "Important",
      "query": "state:open repo:${owner}/${repository} label:important sort:updated-desc"
    },
    {
      "label": "Release",
      "query": "state:open repo:${owner}/${repository} label:release sort:updated-desc"
    }
  ],
  "python.languageServer": "Microsoft",
  "aws.profile": "profile:default"
}
