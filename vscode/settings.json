{
  "editor.minimap.enabled": false,
  "eslint.packageManager": "yarn",
  "explorer.openEditors.visible": 0,
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "[solidity]": {
    "editor.tabSize": 4
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "[sql]": {
    "editor.formatOnSave": false
  },
  "python": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "editor.detectIndentation": false,
  "[json]": {
    "editor.folding": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.schemas": [
    {
      "fileMatch": ["cosmos.config.json"],
      "url": "http://json.schemastore.org/cosmos-config"
    }
  ],
  "editor.rulers": [120],
  "explorer.confirmDragAndDrop": false,
  "editor.folding": false,
  "workbench.editor.enablePreview": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.history": true,
    "**/.retry": true
  },
  "solidity.soliumRules": {
    "imports-on-top": 0,
    "variable-declarations": 0,
    "indentation": ["error", 4],
    "quotes": ["error", "double"]
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "solidity.linter": "solium",
  "solidity.packageDefaultDependenciesContractsDirectory": "",
  "editor.tabSize": 2,
  "breadcrumbs.enabled": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "es": true,
    "lib": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "liveshare.audio.joinCallBehavior": "accept",
  "files.associations": {
    ".env": "shellscript"
  },
  "rust.clippy_preference": "on",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "cSpell.userWords": [
    "Chainable",
    "Etherscan",
    "Mainnet",
    "Rinkeby",
    "angelrf",
    "behaviour",
    "ethereum",
    "goerli",
    "kovan",
    "ropsten"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
    // "source.organizeImports": true
  },
  "explorer.confirmDelete": false,
  "python.jediEnabled": false,
  "editor.formatOnSave": true,
  "todo-tree.tree.showScanModeButton": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "python.formatting.provider": "black",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "githubIssues.queries": [
    {
      "label": "My Issues",
      "query": "default"
    },
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:updated-desc"
    },
    {
      "label": "Milestone: 15",
      "query": "state:open repo:${owner}/${repository} milestone:\"Sprint 15\" sort:updated-desc"
    },
    {
      "label": "Bugs",
      "query": "state:open repo:${owner}/${repository} label:bug sort:updated-desc"
    },
    {
      "label": "Important",
      "query": "state:open repo:${owner}/${repository} label:important sort:updated-desc"
    },
    {
      "label": "Release",
      "query": "state:open repo:${owner}/${repository} label:release sort:updated-desc"
    }
  ],
  "python.languageServer": "Default",
  "aws.profile": "profile:default",
  "eslint.format.enable": true,
  "terminal.external.osxExec": "iTerm.app",
  "liveshare.featureSet": "insiders",
  "java.project.importOnFirstTimeStartup": "automatic",
  "git.suggestSmartCommit": false,
  "githubPullRequests.queries": [
    {
      "label": "Waiting For My Review",
      "query": "is:open review-requested:${user} -label:dependencies -label:\"on hold\""
    },
    {
      "label": "Not Reviewed",
      "query": "is:open -reviewed-by:${user}"
    },
    {
      "label": "Assigned To Me",
      "query": "is:open assignee:${user}"
    },
    {
      "label": "Created By Me",
      "query": "is:open author:${user}"
    },
    {
      "label": "Release",
      "query": "is:open label:release"
    },
    {
      "label": "Leandro's PRs (alfetopito)",
      "query": "is:open author:alfetopito -reviewed-by:${user}"
    },
    {
      "label": "Sasha´s PRs (shoom3301)",
      "query": "is:open author:shoom3301 -reviewed-by:${user}"
    },
    {
      "label": "Tukan´s PRs (tukantje)",
      "query": "is:open author:tukantje -reviewed-by:${user}"
    },
    {
      "label": "Nenad's PRs (nenadV91)",
      "query": "is:open author:nenadV91 -reviewed-by:${user}"
    },
    {
      "label": "FairLight's PRs (fairlighteth)",
      "query": "is:open author:fairlighteth -reviewed-by:${user}"
    }
  ],
  "workbench.iconTheme": "vscode-icons",
  "gitlens.defaultTimeFormat": null,
  "git.autofetch": true,
  "editor.accessibilitySupport": "off",
  "npm.keybindingsChangedWarningShown": true,
  "githubPullRequests.pullBranch": "never",
  "vsicons.dontShowNewVersionMessage": true,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "redhat.telemetry.enabled": false,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "hediet.vscode-drawio.resizeImages": null,
  "jest.autoRun": {
    "watch": false,
    "onSave": "test-only",
    "onStartup": false
  }
}
