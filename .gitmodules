# OhMyZsH: Zsh plugin manager
[submodule "oh-my-zsh"]
	path = oh-my-zsh
	url = https://github.com/robbyrussell/oh-my-zsh.git
  
# PowerLevel10K: OMyZsh Theme
[submodule "zsh-custom/themes/powerlevel10k"]
